<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns:fx="http://javafx.com/fxml" fx:controller="com.example.techstore.Controllers.Admin.DashboardController">
    <padding><Insets top="20" right="20" bottom="20" left="20"/></padding>
    <spacing>10</spacing>

    <Button fx:id="manageProductsButton" text="Manage Products" onAction="#handleManageProducts"/>
    <Button fx:id="manageOrdersButton" text="Manage Orders" onAction="#handleManageOrders"/>
    <Button fx:id="manageCustomersButton" text="Manage Customers" onAction="#handleManageCustomers"/>
    <Button fx:id="manageDeliveriesButton" text="Manage Deliveries" onAction="#handleManageDeliveries"/>
</VBox>
