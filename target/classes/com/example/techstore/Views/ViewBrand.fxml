<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.GridPane?>

<GridPane xmlns:fx="http://javafx.com/fxml" fx:controller="com.example.techstore.Controllers.Admin.brands.ViewBrandController" hgap="10" vgap="10">
    <padding>
        <Insets top="20" right="20" bottom="20" left="20"/>
    </padding>

    <Label text="Brand ID:" GridPane.rowIndex="0" GridPane.columnIndex="0" style="-fx-font-weight: bold;"/>
    <Label fx:id="brandIdLabel" GridPane.rowIndex="0" GridPane.columnIndex="1"/>

    <Label text="Name:" GridPane.rowIndex="1" GridPane.columnIndex="0" style="-fx-font-weight: bold;"/>
    <Label fx:id="nameLabel" GridPane.rowIndex="1" GridPane.columnIndex="1"/>
</GridPane>
